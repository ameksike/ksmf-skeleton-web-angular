{
  "name": "ksmf-web-angular",
  "version": "1.0.0",
  "description": "Template to create web application based on Angular, Angular Material and KsMf",
  "main": "node server/bin/server.js",
  "scripts": {
    "start": "node server/bin/server.js",
    "build": "cd client && npm install && npm run build",
    "test": "cd client && npm run test",
    "dev": "nodemon server/bin/server.js",
    "client:start": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "client:watch": "cd client && npm run watch"    
